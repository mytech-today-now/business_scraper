{
  "name": "business-scraper-app",
  "version": "1.0.0",
  "private": true,
  "description": "A full-stack business web scraping application with React frontend and Node.js backend",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:security": "jest --testPathPattern=security",
    "test:config": "jest --testPathPattern=config",
    "test:features": "jest --testPathPattern=feature-flags",
    "test:cache": "jest --testPathPattern=cache",
    "test:validation": "jest --testPathPattern=validation",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "docs": "echo 'Documentation generation removed due to security vulnerabilities. Use JSDoc comments in code for documentation.'",
    "type-check": "tsc --noEmit",
    "db:migrate": "node database/scripts/migrate.js up",
    "db:migrate:down": "node database/scripts/migrate.js down",
    "db:status": "node database/scripts/migrate.js status",
    "db:reset": "node database/scripts/migrate.js reset",
    "db:setup": "psql -f database/setup.sql",
    "db:sample": "psql -f database/sample_data.sql",
    "config:load": "node scripts/load-config.js load",
    "config:validate": "node scripts/load-config.js validate",
    "config:generate": "node scripts/load-config.js generate",
    "config:summary": "node scripts/load-config.js summary",
    "config:dev": "node scripts/load-config.js generate development .env.development",
    "config:prod": "node scripts/load-config.js generate production .env.production",
    "config:test": "node scripts/load-config.js generate test .env.test",
    "secrets:generate": "node scripts/generate-secrets.js",
    "secrets:prod": "node scripts/generate-secrets.js --env production",
    "secrets:dev": "node scripts/generate-secrets.js --env development",
    "password:generate": "node scripts/generate-password.js",
    "industries:refresh": "node scripts/refresh-industries.js",
    "security-check": "npm audit --audit-level=high",
    "security-ci": "audit-ci --high",
    "security-audit": "npm audit",
    "security-fix": "npm audit fix",
    "security-test": "node scripts/security-test.js",
    "security-test-prod": "TEST_BASE_URL=https://your-domain.com node scripts/security-test.js"
  },
  "dependencies": {
    "@types/pg": "^8.15.4",
    "axios": "^1.6.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dotenv": "^17.2.1",
    "idb": "^8.0.0",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.292.0",
    "next": "^14.0.0",
    "pg": "^8.16.3",
    "playwright": "^1.40.0",
    "puppeteer": "^24.16.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-table": "^7.8.0",
    "react-window": "^1.8.8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@jest/core": "^30.0.5",
    "@jest/environment": "^30.0.5",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-table": "^7.7.0",
    "@types/react-window": "^1.8.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "audit-ci": "^7.1.0",
    "autoprefixer": "^10.4.0",
    "documentation": "^6.2.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "eslint-plugin-security": "^3.0.1",
    "fake-indexeddb": "^4.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jest-environment-node": "^30.0.5",
    "postcss": "^8.4.0"
  }
}
