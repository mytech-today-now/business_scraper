{
  "name": "business-scraper-app",
  "version": "3.5.0",
  "private": true,
  "description": "A full-stack business web scraping application with React frontend and Node.js backend",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:comprehensive": "playwright test src/tests/e2e/comprehensiveUserWorkflows.test.ts",
    "test:e2e:search-engine": "playwright test src/tests/e2e/searchEngineManagement.test.ts",
    "test:e2e:error-handling": "playwright test src/tests/e2e/errorHandlingScenarios.test.ts",
    "test:security": "jest --testPathPattern=security",
    "test:security:vulnerability": "jest --testPathPattern=vulnerabilityScanning",
    "test:security:penetration": "jest --testPathPattern=penetrationTesting",
    "test:performance": "jest --testPathPattern=performance",
    "test:performance:load": "jest --testPathPattern=loadTesting",
    "test:performance:regression": "jest --testPathPattern=performanceRegression",
    "test:accessibility": "playwright test src/tests/accessibility/",
    "test:accessibility:wcag": "playwright test src/tests/accessibility/wcagCompliance.test.ts",
    "test:config": "jest --testPathPattern=config",
    "test:features": "jest --testPathPattern=feature-flags",
    "test:cache": "jest --testPathPattern=cache",
    "test:validation": "jest --testPathPattern=validation",
    "test:comprehensive": "npm run test:unit:comprehensive && npm run test:integration:comprehensive && npm run test:system:comprehensive && npm run test:regression:comprehensive",
    "test:unit:comprehensive": "jest --testPathPattern=unit/comprehensive",
    "test:integration:comprehensive": "jest --testPathPattern=integration/comprehensive",
    "test:system:comprehensive": "jest --testPathPattern=system/comprehensive",
    "test:regression:comprehensive": "jest --testPathPattern=regression/comprehensive",
    "test:acceptance:comprehensive": "playwright test src/tests/acceptance/",
    "test:compatibility:comprehensive": "playwright test src/tests/compatibility/",
    "test:exploratory:comprehensive": "jest --testPathPattern=exploratory/comprehensive",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:security && npm run test:performance && npm run test:accessibility && npm run test:comprehensive",
    "docs": "echo 'Documentation: Use JSDoc comments in code. Consider installing typedoc for TypeScript documentation or @storybook/react for component docs.'",
    "type-check": "tsc --noEmit",
    "db:migrate": "node database/scripts/migrate.js up",
    "db:migrate:down": "node database/scripts/migrate.js down",
    "db:status": "node database/scripts/migrate.js status",
    "db:reset": "node database/scripts/migrate.js reset",
    "db:setup": "psql -f database/setup.sql",
    "config:load": "node scripts/load-config.js load",
    "config:validate": "node scripts/load-config.js validate",
    "config:generate": "node scripts/load-config.js generate",
    "config:summary": "node scripts/load-config.js summary",
    "config:dev": "node scripts/load-config.js generate development .env.development",
    "config:prod": "node scripts/load-config.js generate production .env.production",
    "config:test": "node scripts/load-config.js generate test .env.test",
    "secrets:generate": "node scripts/generate-secrets.js",
    "secrets:prod": "node scripts/generate-secrets.js --env production",
    "secrets:dev": "node scripts/generate-secrets.js --env development",
    "password:generate": "node scripts/generate-password.js",
    "industries:refresh": "node scripts/refresh-industries.js",
    "security-check": "npm audit --audit-level=high",
    "security-ci": "audit-ci --high",
    "security-audit": "npm audit",
    "security-fix": "npm audit fix",
    "security-test": "node scripts/security-test.js",
    "security-test-prod": "TEST_BASE_URL=https://your-domain.com node scripts/security-test.js"
  },
