# E2E Test Environment Configuration - EXAMPLE TEMPLATE
# Business Scraper App - Next.js 14 with Adapted MVC Architecture
# 
# IMPORTANT: This is a template file with placeholder values.
# Copy this file to e2e-test.env and replace placeholders with real values.
# The real e2e-test.env file will be automatically ignored by .gitignore
#
# Architecture: Model (src/model/) | View (src/view/) | Controller (src/controller/)
# Tech Stack: Next.js 14, React 18, TypeScript, Puppeteer, Jest, Playwright
# Testing: Jest (80% coverage) + Playwright E2E + Integration + Security Tests

# Application Configuration
NEXT_PUBLIC_APP_NAME=Business Scraper App (E2E Test)
NEXT_PUBLIC_APP_VERSION=1.3.0-e2e-test
NODE_ENV=test
NEXT_PUBLIC_DEBUG=false
PORT=3002

# Database Configuration (Test)
# Use IndexedDB for fast test execution and isolation
DB_TYPE=indexeddb
DB_HOST=localhost
DB_PORT=5432
DB_NAME=business_scraper_e2e_test
DB_USER=postgres
DB_PASSWORD=postgres
DB_POOL_MIN=1
DB_POOL_MAX=3
DB_POOL_IDLE_TIMEOUT=10000
DB_CONNECTION_TIMEOUT=5000
DB_SSL=false

# Security Configuration (E2E Test)
# Enable authentication for E2E testing
ENABLE_AUTH=true
SESSION_TIMEOUT=1800000
MAX_LOGIN_ATTEMPTS=20
LOCKOUT_DURATION=60000
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX=10000
SCRAPING_RATE_LIMIT=1000

# Authentication (E2E Test)
# REPLACE WITH REAL CREDENTIALS FOR LOCAL TESTING
ADMIN_USERNAME=REPLACE_WITH_REAL_ADMIN_USERNAME
ADMIN_PASSWORD=REPLACE_WITH_REAL_ADMIN_PASSWORD
# Use hashed password for security
ADMIN_PASSWORD_HASH=REPLACE_WITH_REAL_PASSWORD_HASH
ADMIN_PASSWORD_SALT=REPLACE_WITH_REAL_PASSWORD_SALT

# Scraping Configuration (Test)
# Fast settings optimized for test execution speed
SCRAPING_TIMEOUT=5000
SCRAPING_MAX_RETRIES=1
SCRAPING_DELAY_MS=100
BBB_SCRAPING_DELAY_MS=100
SEARCH_ENGINE_TIMEOUT=5000
MAX_SEARCH_RESULTS=10
SCRAPING_MAX_PAGES_PER_SITE=3
SCRAPING_DEPTH=2

# Multi-Strategy Search Engine Configuration (Test)
# Simplified configuration for testing
ENABLE_BBB_DISCOVERY=true
ENABLE_INDUSTRY_EXPANSION=true
ENABLE_ZIP_RADIUS_VALIDATION=true
BBB_SEARCH_TYPE=all_businesses
ZIP_RADIUS_MILES=10

# API Keys (Test)
# REPLACE WITH REAL API KEYS FOR COMPREHENSIVE E2E TESTING
# NEVER COMMIT REAL API KEYS TO VERSION CONTROL!
AZURE_AI_FOUNDRY_API_KEY=REPLACE_WITH_REAL_AZURE_API_KEY
AZURE_AI_FOUNDRY_ENDPOINT=REPLACE_WITH_REAL_AZURE_ENDPOINT
AZURE_AI_FOUNDRY_REGION=REPLACE_WITH_REAL_AZURE_REGION
GOOGLE_MAPS_API_KEY=REPLACE_WITH_REAL_GOOGLE_MAPS_KEY
GOOGLE_CUSTOM_SEARCH_API_KEY=REPLACE_WITH_REAL_GOOGLE_SEARCH_KEY
GOOGLE_SEARCH_ENGINE_ID=REPLACE_WITH_REAL_SEARCH_ENGINE_ID
OPENCAGE_API_KEY=REPLACE_WITH_REAL_OPENCAGE_KEY
BING_SEARCH_API_KEY=REPLACE_WITH_REAL_BING_KEY
YANDEX_SEARCH_API_KEY=REPLACE_WITH_REAL_YANDEX_KEY

# Cache Configuration (Test)
# Use memory cache for testing
CACHE_TYPE=memory
CACHE_MAX_SIZE=100
CACHE_TTL=60000

# Logging Configuration (Test)
LOG_LEVEL=warn
LOG_FORMAT=text
LOG_ENABLE_CONSOLE=false
LOG_ENABLE_FILE=false
LOG_FILE_PATH=./logs/e2e-test.log
LOG_MAX_FILE_SIZE=1048576
LOG_MAX_FILES=1

# Data Export Configuration (Test)
# Limited export for faster test execution
EXPORT_ENABLE_CSV=true
EXPORT_ENABLE_XLSX=true
EXPORT_ENABLE_PDF=false
EXPORT_ENABLE_JSON=true
EXPORT_MAX_RECORDS=100

# Testing Configuration
# Comprehensive test coverage requirements (90% threshold for E2E)
TEST_COVERAGE_THRESHOLD=90
TEST_TIMEOUT=30000
E2E_TEST_TIMEOUT=60000
ENABLE_TEST_MOCKS=true
ENABLE_UNIT_TESTS=true
ENABLE_INTEGRATION_TESTS=true
ENABLE_E2E_TESTS=true
ENABLE_SECURITY_TESTS=true
ENABLE_FEATURE_TESTS=true

# Feature Flags (Test)
# Selective feature enabling for comprehensive testing
FEATURE_ENABLE_CACHING=false
FEATURE_ENABLE_RATE_LIMITING=false
FEATURE_ENABLE_METRICS=false
FEATURE_ENABLE_EXPERIMENTAL=true
FEATURE_ENABLE_BBB_INTEGRATION=true
FEATURE_ENABLE_CHAMBER_OF_COMMERCE=true
FEATURE_ENABLE_YELP_SCRAPING=true
FEATURE_ENABLE_DOMAIN_BLACKLIST=true
FEATURE_ENABLE_PER_INDUSTRY_BLACKLIST=true

# Test-specific settings
NEXT_TELEMETRY_DISABLED=1
CI=true
ENABLE_TEST_COVERAGE_REPORTS=true
ENABLE_MOCK_SERVICES=true

